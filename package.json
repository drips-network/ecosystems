{
  "name": "ecosystems-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "check": "tsc --noEmit",
    "dev": "ts-node src/index.ts",
    "start": "node build/src/index.js",
    "db:create-database": "node build/scripts/create-database.js",
    "db:run-migrations": "node build/scripts/run-migrations.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "dev:db:run-migrations": "npm run clean && npm run compile && node build/scripts/run-migrations.js",
    "dev:db:create-migration": "typeorm-ts-node-commonjs migration:generate src/common/infrastructure/migrations/$npm_config_name -d src/common/infrastructure/dataSource.ts",
    "dev:db:revert-migration": "typeorm-ts-node-commonjs migration:revert -d src/common/infrastructure/dataSource.ts",
    "dev:db:log-pending-migrations": "typeorm-ts-node-commonjs schema:log -d src/common/infrastructure/dataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@efstajas/versioned-parser": "^0.1.4",
    "@octokit/rest": "^21.1.0",
    "@pinata/sdk": "^2.1.0",
    "bee-queue": "^1.7.1",
    "bull-arena": "^4.4.2",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "ethers": "^6.13.5",
    "express": "^4.21.2",
    "ioredis": "^5.4.2",
    "pg": "^8.13.1",
    "pg-connection-string": "^2.7.0",
    "pgtools": "^1.0.1",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "xstate": "^5.19.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bull-arena": "^3.0.10",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "abitype": "^1.0.8",
    "gts": "^6.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
